@using YogaStudio.App.Models.ViewModels
@model ICollection<OrderViewModel>

<div class="table-responsive">
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>Order number</th>
                <th>Date of order</th>
                <th>Client</th>
                <th>Courses</th>
                <th>Payment type</th>
                <th>Total amount</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(order => order)
        </tbody>
    </table>
</div>

