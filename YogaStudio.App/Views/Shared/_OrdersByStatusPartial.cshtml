@using YogaStudio.App.Models.ViewModels
@model ICollection<MyOrderViewModel>
<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Order number</th>
                <th>Date of order</th>
                <th>Total amount</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(myOrder => myOrder)
        </tbody>
    </table>
</div>
